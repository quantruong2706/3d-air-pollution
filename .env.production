# Production environment variables
# These override values in the base .env file

# API Configuration
VITE_API_URL=https://api.production.airpollution.com/v1
VITE_API_KEY=${API_KEY} # Will be replaced during deployment

# Application Environment
VITE_ENV=production

# Feature Flags
VITE_ENABLE_LOGGING=false

# Map Configuration
VITE_MAP_PROVIDER=azure
VITE_DATA_SOURCE=remote